{
  "name": "react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite-node server.ts",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src/*.tsx",
    "format": "prettier --write .",
    "test": "vitest run --coverage",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:report": "npx nyc report --reporter=text",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:run-e2e",
    "test:e2e": "npm run cy:e2e && npm run cy:report"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.11.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "msw": "^1.2.1",
    "prettier": "^2.6.2",
    "sass": "^1.59.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-node": "^0.30.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A ."
    }
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
